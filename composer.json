{
    "name": "entanet/entanet-behat",
    "description": "A package to help Entanet's QA department automate features with Behat.",
    "type": "extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Ralphs",
            "email": "ryan.ralphs@enta.net",
            "role": "SDET"
        },
        {
            "name": "Tomos Lloyd",
            "email": "tomos.lloyd@enta.net",
            "role": "Software Development Manager"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "behat/behat": "^3.5",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.3",
        "imbo/behat-api-extension": "^2.1",
        "laracasts/behat-laravel-extension": "^1.1",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0",
        "superbalist/php-pubsub": "^2.0.0",
        "superbalist/laravel-pubsub": "^3.0.0",
        "behat/mink-selenium2-driver": "^1.2",
        "doctrine/dbal": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Entanet\\Behat\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Entanet\\Behat\\BehatServiceProvider"
            ]
        }
    }
}
