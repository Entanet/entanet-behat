{
    "name": "entanet/entanet-behat",
    "description": "A package to help Entanet's QA department automate features with Behat.",
    "type": "extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Ralphs",
            "email": "ryan.ralphs@cityfibre.com",
            "role": "SDET - Author / Maintainer"
        },
        {
            "name": "Tomos Lloyd",
            "email": "tomos.lloyd@cityfibre.com",
            "role": "Software Architect / Contributor / Maintainer"
        },
        {
            "name": "Contributors",
            "email": "adam.humphreys@cityfibre.com",
            "role": "Various contributions such as version updates and minor bug fixes."
        }
    ],
    "require": {
        "php": "^7.1.3|8",
        "behat/behat": "^3.5",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.3",
        "imbo/behat-api-extension": "*",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": ">=6.0",
        "superbalist/php-pubsub": "^2.0.0",
        "entanet/laravel-pubsub": "dev-laravel-8-update",
        "behat/mink-selenium2-driver": "^1.2",
        "doctrine/dbal": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Entanet\\Behat\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Entanet\\Behat\\BehatServiceProvider"
            ]
        }
    }
}
